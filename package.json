{
  "name": "projet",
  "version": "1.0.0",
  "description": "Projet Tiger Discord Bot",
  "main": "main.js",
  "repository": "https://github.com/keketiger/tiger-discordbot.git",
  "author": "keketiger <pro@keketiger.fr>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-config-aqua": "^6.0.2"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error", 
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error", 
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off"
    }
  }
}
